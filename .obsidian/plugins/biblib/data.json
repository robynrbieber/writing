    {
      "name": "year",
      "template": "{{year}}",
      "enabled": true
    },
    {
      "name": "dateCreated",
      "template": "{{currentDate}}",
      "enabled": true
    },
    {
      "name": "reading-status",
      "template": "to-read",
      "enabled": true
    },
    {
      "name": "aliases",
      "template": "[\"{{title|sentence}}\"]",
      "enabled": true
    },
    {
      "name": "author-links",
      "template": "[{{#authors}}\"[[Author/{{.}}]]\",{{/authors}}]",
      "enabled": true
    },
    {
      "name": "attachment",
      "template": "[{{#attachments}}{{.}},{{/attachments}}]",
      "enabled": true
    },
    {
      "name": "related",
      "template": "[{{links}}]",
      "enabled": true
    }
